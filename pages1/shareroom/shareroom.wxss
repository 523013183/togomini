/* pages1/shareroom/shareroom.wxss */
page{
  background-color: #fff;
  overflow: hidden;
}
.c-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  padding: 0 24rpx;
  width: 100%;
  box-sizing: border-box;
}

.c-item {
  width: 100%;
  height: 400rpx;
  margin-bottom: 20rpx;
  display: inline-block;
  position: relative;
}

.c-item image {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
  top: 0;

}

.bottom2 {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 60rpx;
  width: 100%;
  background-color: rgba(0,0,0,.5);
  padding: 30rpx 17rpx;
  box-sizing: border-box;
  position: absolute;
  z-index: 5;
  bottom: 0;
}

.bottom2>view{
  color: #fff;
  font-size: 26rpx;
  font-weight: bold;
}

.a-img {
  width: 100%;
  height: 410rpx;
}
.wdith{
  max-width: 250rpx;
}
scroll-view{
   width: 100%;
}
.head{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  background-color: #f2f2f2;
  justify-content: center;

}
.saerch{
  display: flex;
  align-items: center;
  background-color: #fff;
  width: 50%;
  padding: 0 15rpx;
  border-radius: 40rpx;
}
.saerch>image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.saerch>input{
  height: 60rpx;
}
.saerchbtn{
  margin-left: 30rpx;
  width: 120rpx;
  height: 60rpx;
  background-color: #f9e15c;
  font-size: 24rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 40rpx;
}
.none{
  margin-top: 300rpx;
  text-align: center;
}
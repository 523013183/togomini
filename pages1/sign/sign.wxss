.head-bg{
  background-color: rgb(120,190,255);
  width: 100%;
  height: 400rpx;
  position: absolute;
  z-index: -1;
  top: 0;
}
.sign-btn{
  width: 200rpx;
  height: 80rpx;
  border-radius: 50rpx;
  text-align: center;
  line-height: 80rpx;
  border: 1rpx solid #fff;
  color: #fff;
  margin: 30rpx auto 20rpx;
  font-size: 30rpx;
}
.txt-{
  text-align: center;
  color: #fff;
  font-size: 26rpx;
}
.data-{
  box-shadow: 10rpx 10rpx 50rpx #f2f2f2;
  width: 700rpx;
  background-color: #fff;
  border-radius: 20rpx;
  margin: 40rpx auto;
  padding: 20rpx;
  box-sizing: border-box;
}
.data-title{
  border-bottom: 1rpx solid #f2f2f2;
  text-align: center;
  padding-bottom: 10rpx;
  margin-bottom: 20rpx;
}
.hebdomad{
  display: flex;
  align-items: center;
  width: 100%;
  flex-wrap: wrap;
  margin-bottom: 10rpx;
  margin-left: 10rpx;
}
.hebdomad text{
  width: 90rpx;
  text-align: center;
}
.day-text{
  margin: 10rpx 0;
}
.rule-title{
  text-align: center;

}
page{
  background-color: #fff;
}
.list-item{
  font-size: 26rpx;
  margin: 20rpx  70rpx;
  position: relative;
}
.list-item::after{
  position: absolute;
  content: '';
  left: -30rpx;
  top: 10rpx;
  border-radius: 50%;
  background-color: rgb(120,190,255);
  width: 20rpx;
  height: 20rpx;
}
.color{
  color: rgb(120,190,255);
  font-weight: bold;
}
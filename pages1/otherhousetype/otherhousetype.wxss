/* pages1/otherhousetype/otherhousetype.wxss */

page {
  background-color: #fff;
  overflow: hidden;
}

.item {
  width: 700rpx;
  height: 200rpx;
  margin: 20rpx 25rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0 20rpx;
  border: 1rpx solid #f2f2f2;
  position: relative;
}

.left {
  width: 150rpx;
  height: 150rpx;
}

.right {
  height: 150rpx;
  width: 450rpx;
  margin-left: 20rpx;
}

.title {
  margin-bottom: 20rpx;
}

.direction, .acreage {
  font-size: 24rpx;
  margin-bottom: 10rpx;
}

.position {
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  font-size: 24rpx;
}

scroll-view {
  width: 100%;
}

.head {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  background-color: #f2f2f2;
  justify-content: center;
}

.saerch {
  display: flex;
  align-items: center;
  background-color: #fff;
  width: 50%;
  padding: 0 15rpx;
  border-radius: 40rpx;
}

.saerch>image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

.saerch>input {
  height: 60rpx;
}

.saerchbtn {
  margin-left: 30rpx;
  width: 120rpx;
  height: 60rpx;
  background-color: #f9e15c;
  font-size: 24rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 40rpx;
}

.none {
  margin-top: 300rpx;
  text-align: center;
}

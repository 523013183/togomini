page{
	background-color: #fafafa;
}
.proof_part{
	margin: 40rpx;
	padding: 40rpx 30rpx;
	font-size: 26rpx;
	border-radius: 15rpx;
	box-shadow: 0 0 10px #ccc;
	background-color: #FFFFFF;
}
.proof_part .title{
	font-size: 30rpx;
	font-weight: bold;
}
.proof_part .activity_time{
	margin: 20rpx 0 25rpx;
	color: #666666;
}
.proof_part .activity_pic{
	width: 100%;
	height: 365rpx;
}
.proof_part .erwei_code{
	margin: 30rpx 0 35rpx;
	text-align: center;
}
.proof_part .apply_person{
	color: #666666;
}
.proof_part .apply_person .name{
	margin-bottom: 10rpx;
}
.apply_proof > button{
	width: 250rpx;
	height: 70rpx;
	line-height: 70rpx;
	font-size: 30rpx;
	text-align: center;
	border-radius: 35rpx;
	background-color: #F9E15C;
}


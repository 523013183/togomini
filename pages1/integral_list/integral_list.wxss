page {
  background-color: #fff;
}

.my_integral {
  display: flex;
  align-items: center;
  width: 100%;
  height: 100rpx;
  background-color: rgb(47, 155, 254);
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.my_integral .left {
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
}

.my_integral .right {
  color: #fff;
  font-size: 24rpx;
}

.list {
  padding: 0 20rpx;
}

.item {
  width: 100%;
  height: 130rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15rpx 0;
  box-sizing: border-box;
  border-bottom: 1rpx solid #ccc;
}

.item .left {
  display: flex;
  flex-direction: column;
}

.item .left text {
  font-size: 28rpx;
  font-weight: bold;
  color: #000;
  margin-bottom: 10rpx;
}

.text {
  font-weight: normal !important;
  font-size: 24rpx !important;
  color: #ccc !important;
}

.item right {
  font-size: 28rpx;
  color: #000;
}

page {
  overflow: hidden;
}
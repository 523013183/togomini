	background-color: #f2be24;
}
.color3{
	background-color: #886c69;
}
.color4{
	background-color: #c6ac87;
}
.color5{
	background-color: #7d87b2;
}
.swiper{
  width: 668rpx;
	height: 400rpx;
  margin: 20rpx auto;
  position: relative;
}

.dots{ 
  position: absolute; 
  left: 0; 
  right: 0; 
  bottom: 20rpx; 
  display: flex; 
  justify-content: center; 
} 
.dots .dot{ 
  margin: 0 8rpx; 
  width: 14rpx; 
  height: 14rpx; 
  background: #fff; 
  border-radius: 8rpx; 
  transition: all .6s; 
} 
.dots .dot.active{ 
  width: 54rpx; 
  background: #f9e15c; 
}
.title{
  margin-left: 40rpx;
  font-size: 36rpx;
  margin-bottom: 20rpx;
}
.bottom{
  width: 670rpx;
  text-align: center;
  position: absolute;
  bottom: 20rpx;
  color: #ccc;
}
.backme{
  width: 70rpx;
  height: 70rpx;
  position: fixed;
  right: 30rpx;
  top: 560rpx;

}
.product_price{
  display: flex;
  justify-content: space-between;
}
.product_price view{
  background-color: #f9e15c;
  padding: 2rpx 20rpx;
  border-radius: 50rpx;
}

.collect{
  position: fixed;
  bottom: 350rpx;
  right: 20rpx;
  background-color: #f9e15c;
  text-align: center;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  line-height: 80rpx;
  font-weight: bolder;
  font-size: 20rpx;
}
}

.ceshi_prew text {
  width: 25rpx;
  height: 25rpx;
  border: 1rpx solid #000;
  border-width: 5rpx 5rpx 0 0;
  transform: rotate(-135deg);
  display: inline-block;
}

.ceshi_next text {
  width: 25rpx;
  height: 25rpx;
  border: 1rpx solid #000;
  border-width: 5rpx 5rpx 0 0;
  transform: rotate(45deg);
  display: inline-block;
}

.ceshi_next {
  width: 50rpx;
  height: 50rpx;

.scroll-view1 {
  width: 100%;
  height: 100rpx;
  background-color: #f2f2f2;
  white-space: nowrap;
  padding: 10rpx;
  box-sizing: border-box;
}

.lunboimg {
  width: 100rpx;
  height: 100rpx;
  margin-right: 15rpx;
}

.roomdate {
  background-color: #fff;
  width: 100%;
  padding: 30rpx 40rpx;
  box-sizing: border-box;
}

.itemnum {
  display: flex;
  flex-wrap: wrap;
}

.roomname {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 30rpx;
}

.roomname>text {
  font-size: 28rpx;
}

.blue {
  color: rgb(109, 179, 249);
}

.roomitem {
  display: flex;
  align-items: center;
  width: 50%;
  margin-bottom: 20rpx;
}

.roomitem>view {
  font-size: 24rpx;
}

.roomitem>view:nth-child(1) {
  font-weight: bolder;
  width: 150rpx;
}

.red {
  color: red;
}

.c-title {
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 0 40rpx 0 20rpx;
  box-sizing: border-box;
}

.c-title>view:nth-child(1) {
  border-left: 4rpx solid #2ea9df;
  padding-left: 15rpx;
  font-weight: bolder;
}

.scroll-view2 {
  width: 100%;
  white-space: nowrap;
  padding: 0 20rpx;
  box-sizing: border-box;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
}

.s-item {
  width: 90%;
  display: inline-block;
  margin-right: 50rpx;
}

.s-img {
  width: 100%;
  height: 400rpx;
  border-radius: 20rpx;
}

.s-bottom {
  width: 100%;
  display: flex;
  align-items: center;
}

.sb-img {
  width: 130rpx;
  height: 130rpx;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #f2f2f2;
  margin: 10rpx;
}

.sb-img image {
  width: 80rpx;
  height: 80rpx;
}

.stlylist {
  background-color: #f2f2f2;
  width: 100%;
  padding: 10rpx 40rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}

.stlylistitem {
  width: 100%;
  display: flex;
  padding: 20rpx 20rpx 0 20rpx;
  box-sizing: border-box;
  position: relative;
  margin-bottom: 15rpx;
}

.avater {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.stlylistdate {
  display: flex;
  flex-direction: column;
}

.stlylistdate>text {
  font-size: 24rpx;
  margin-bottom: 5rpx;
}

.phone {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 90rpx;
  top: 10rpx;
}

.wechat {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
}

.font {
  font-size: 30rpx !important;
  font-weight: bold;
}

.foota {
  width: 100%;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  padding-bottom: env(safe-area-inset-bottom);
  left: 0;
  z-index: 5;
}

.join {
  width: 60%;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 24rpx;
  background-color: #ffe252;
  border-radius: 10rpx;
  margin: 25rpx 20%;
  border: 1rpx solid #f2f2f2;
}

.exclusive {
  width: 80rpx;
  height: 80rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 80rpx;
  background-color: #ffe252;
  border-radius: 50%;
  border: 1rpx solid #f2f2f2;
  position: absolute;
  right: 20rpx;
  top: 10rpx;
}

.tab {
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: center;
}

.tab>view {
  width: 150rpx;
  height: 50rpx;
  border: 1rpx solid #ccc;
  text-align: center;
  line-height: 50rpx;
  font-size: 24rpx;
  margin: 0 10rpx;
  border-radius: 10rpx;
}





page {
  background-color: #fff;
}

.warehouse .title {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 30rpx 0;
  font-size: 25rpx;
  background-color: #FFFFFF;
  position: fixed;
  top: 0;
  width: 100%;
  box-sizing: border-box;
  z-index: 9;
}

.warehouse .title .item {
  width: 180rpx;
  height: 65rpx;
  line-height: 65rpx;
  text-align: center;
  font-weight: bold;
  border-radius: 32.5rpx;
  background-color: #f9e15c;
  margin-right: 10rpx;
}

.choose_item {
  /* position: relative; */
}

.choose-item-name {
  max-width: 114rpx;
  overflow: hidden;
  white-space: nowrap;
  text-align: center;
}

.chosoe-all {

  position: absolute;
  display: block;
  background: white;
  top: 88rpx;
  line-height: 80rpx;
  max-height: 400rpx;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
  font-size: 28rpx;
  padding: 20rpx 10rpx;
  /* text-align: center; */
  width: 100%;
  right: 0;
}

.chosoe-all view {
  font-size: 24rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
}

.warehouse .title .search_box {
  display: flex;
  align-items: center;
  width: calc(100% - 200rpx);
  height: 65rpx;
  line-height: 65rpx;
  padding: 0 15rpx;
  border-radius: 32.5rpx;
  background-color: #F2F2F2;
}

.warehouse .title .search_box>image {
  margin-right: 10rpx;
}

.warehouse .choose_wrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0;
  background-color: #FFFFFF;
  position: fixed;
  top: 93rpx;
  width: 100%;
  box-sizing: border-box;
  z-index: 9;
}

.warehouse .choose_wrapper .choose_item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
}

.warehouse .choose_wrapper .choose_item>image {
  margin-left: 10rpx;
}

.warehouse .product {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 25rpx 40rpx 0;
}

.warehouse .product .product_item {
  width: calc(50% - 10rpx);
  margin-bottom: 55rpx;
  font-size: 25rpx;
}

.warehouse .product .product_item .product_img {
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  background-color: #f4f4f4;
}

.warehouse .product .product_item .pro_desc {
  padding: 20rpx;
  font-size: 24rpx;
  background-color: #FFFFFF;
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}

.warehouse .product .product_item .pro_desc .product_name {
  font-size: 26rpx;
  margin-bottom: 20rpx;
  width: 100%;
  /* font-weight: bold; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.warehouse .product .product_item .pro_desc .product_price {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.warehouse .product .product_item .pro_desc .product_price .price {
  color: #E93323;
  font-weight: bold;
}

.warehouse .product .product_item .pro_desc .product_price .add_list {
  width: 140rpx;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  border-radius: 22.5rpx;
  background-color: #F9E15C;
}

.warehouse .list_mune {
  position: fixed;
  bottom: 30%;
  right: 30rpx;
}




.zhe {
  background-color: rgba(0, 0, 0, .7);
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tan {
  background-color: #fff;
  width: 460rpx;
  height: 280rpx;
  border-radius: 10rpx;
  text-align: center;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  position: fixed;
  left: 145rpx;
  top: 400rpx;
  z-index: 99;
}

.tan view {
  font-size: 25rpx;
}

.tan input {
  border: 1rpx solid #F2F2F2;
  background-color: #F2F2F2;
  margin: 30rpx 0;
  font-size: 20rpx;
  height: 60rpx;
  width: 150rpx;
}

.tan .text {
  display: inline-block;
  background-color: #F9E15C;
  padding: 8rpx 30rpx;
  border-radius: 50rpx;
  font-size: 25rpx;
  margin-top: 20rpx;
}

.add {
  height: 60rpx;
  width: 60rpx;
  border: 1px solid #F2F2F2;
  font-size: 50rpx;
  display: inline-block;
  text-align: center;
  line-height: 60rpx;
}

.txt2 {
  display: flex;
  flex-direction: row;
  width: 270rpx;
  height: 60rpx;
  align-items: center;
  justify-content: space-between;
  margin: 40rpx 75rpx 20rpx;
}

.none {
  font-size: 30rpx;
  color: #cccc;
  text-align: center;
  margin-top: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.choose {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background-color: #fff;
  z-index: 9;
}

.item {
  padding: 0 40rpx;
}

.item .title {
  font-weight: bold;
  margin-bottom: 25rpx;
}

.pricea {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.pricea input {
  width: 280rpx;
  background-color: rgb(240, 240, 240);
  text-align: center;
  border-radius: 10rpx;
  height: 50rpx;
}

.pricea view {
  width: 50rpx;
  height: 5rpx;
  background-color: #000;
}

.priceb {
  display: flex;
  align-items: center;
  margin: 20rpx 0;
  justify-content: space-between;
}

.priceb view {
  background-color: rgb(240, 240, 240);
  width: 210rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 5rpx;
  font-size: 24rpx;
}

.gray {
  width: 100%;
  height: 20rpx;
  background-color: rgb(240, 240, 240);
  border-top: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  margin: 50rpx 0 20rpx 0;
}

.style {
  padding: 0 40rpx;
  margin-bottom: 30rpx;
}

.style .title {
  font-weight: bold;
  margin-bottom: 25rpx;
}

.style .list {
  display: flex;
  flex-wrap: wrap;
}

.style .list view {
  width: 210rpx;
  height: 50rpx;
  background-color: rgb(240, 240, 240);
  text-align: center;
  line-height: 50rpx;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  margin-left: 6rpx;
  margin-right: 6rpx;
  font-size: 26rpx;
}

.btn {
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 20rpx;
}

.cz {
  width: 300rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  background-color: #fff;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
}

.qr {
  width: 300rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  background-color: rgb(249, 225, 92);
  border-radius: 10rpx;
  border: 1rpx solid #ccc;
}

.close {
  position: absolute;
  top: 20rpx;
  right: 40rpx;
  width: 100rpx;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  background-color: rgb(240, 240, 240);
}

.active {
  background-color: #f9e15c !important;
}

.plusprice_tan {
  background-color: rgba(0, 0, 0, .6);
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  z-index: 10;
}

.plusprice {
  width: 500rpx;
  height: 300rpx;
  background-color: #fff;
  border-radius: 10rpx;
  position: absolute;
  top: 350rpx;
  left: 125rpx;
  padding: 20rpx 40rpx;
}

.plusprice .title {
  text-align: center;
  font-weight: bolder;

}

.flex {
  display: flex;
  align-items: center;
  height: 80rpx;
  border: 1rpx solid #f2f2f2;
  width: 350rpx;
  margin: 30rpx
}

.flex input {
  width: 150rpx;
  height: 80rpx;
  text-align: center;
  font-size: 24rpx;
}

.flex view {
  width: 98rpx;
  height: 78rpx;
  background-color: #fff;
  text-align: center;
  line-height: 80rpx;
  font-size: 40rpx;
}

.plusprice_btn {
  width: 200rpx;
  height: 60rpx;
  margin: 0 auto;
  text-align: center;
  line-height: 60rpx;
  background-color: #f9e15c;
  border-radius: 50rpx;
}
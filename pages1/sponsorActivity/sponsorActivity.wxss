	background-color: #f3f6fb;
}
.sponsor_activity .activity_title .tel_code{
	display: flex;
	align-items: center;
}
.sponsor_activity .activity_title .tel_code >input{
	margin-left: 30rpx;
}
.sponsor_activity >button{
	width: 90%;
	height: 90rpx;
	line-height: 90rpx;
	text-align: center;
	font-size: 36rpx;
	margin: 40rpx auto 35rpx;
	border-radius: 45rpx;
	background-color: #F9E15C;
}


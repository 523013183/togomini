/* 商品关联方案样式 */
.scheme-search-box {
	width: 100%;
	height: 100%;
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: space-between;
	margin-right: 20rpx;
}
.scheme-search-box input {
	flex: 1;
	text-align: right;
}
.scheme-search-btn{
	border-radius: 0;
	width: 90rpx;
	height: 100%;
}
.scheme-choose {
	padding: 20rpx 0;
}
.scheme-choosed {
	font-size: 28rpx;
	padding: 0 30rpx;
}
.scheme-choose scroll-view {
	width: 100%;
	min-height: 132rpx;
	max-height: 376rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
	background-color: #fff;
	margin-top: 10rpx;
}
.scheme-lab-item {
	display: flex;
	width: 100%;
	height: 112rpx;
	overflow: hidden;
	margin: 10rpx 0;
	flex-direction: row;
	align-items: center;
	justify-content: space-between;
}
.scheme-item-img {
	width: 122rpx;
	height: 112rpx;
	background-color: #eee;
}
.scheme-item-name {
	flex: 1;
	box-sizing: border-box;
	padding: 0 20rpx;
	font-size: 28rpx;
}
.scheme-checkbox {
	height: 43rpx;
	width: 43rpx;
}
.scheme-checkbox image {
	width: 100%;
	height: 100%;
}
.picker-box view {
	color: #2ea9df;
	text-align: right;
}
.active-address {
	flex: 1;
	max-width: 520rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.icon-address {
	width:32rpx;
	height:32rpx;
}
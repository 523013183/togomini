.sponsor_activity .activity_title{
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding: 30rpx;
	font-size: 30rpx;
	border-bottom: 1px solid #F6F6F6;
	background-color: #FFFFFF;
}
.sponsor_activity .activity_title .right{
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.sponsor_activity .activity_title >input{
	text-align: right;
}
.sponsor_activity .activity_title .right >input{
	text-align: right;
}
.sponsor_activity .activity_title .right >image{
	margin-left: 10rpx;
}
.sponsor_activity .activity_end{
	margin-bottom: 20rpx;
}
.sponsor_activity .sponsor_necessary{
	padding: 35rpx 30rpx 45rpx; 
	font-size: 26rpx;
	margin-bottom: 20rpx;
	background-color: #FFFFFF;
}
.sponsor_activity .sponsor_necessary .title{
	font-size: 30rpx;
}
.sponsor_activity .sponsor_necessary .item{
	display: flex;
	/* justify-content: space-between; */
	flex-direction: row;
	flex-wrap: wrap;
	margin: 35rpx 0 25rpx;
}
.sponsor_activity .sponsor_necessary .item .necessary{
	color: #f9e15c;
	border: 1px solid #f9e15c;
}
.sponsor_activity .sponsor_necessary .item > text{
	display: inline-block;
	/* width: calc((100% - 100rpx)/5); */
  padding: 0 30rpx;
	height: 55rpx;
	line-height: 55rpx;
	text-align: center;
	margin-right: 25rpx;
	margin-bottom: 15rpx;
	border: 1px solid #c2c2c2;
	border-radius: 27.5rpx;
}
.sponsor_activity .sponsor_necessary .item > text:nth-child(5n){
	margin-right: 0rpx;
}
.sponsor_activity .sponsor_necessary .make_self{
	color: #b8b8b8;
}
.sponsor_activity .sponsor_necessary .make_self .add_btn{
	display: inline-block;
	width: 205rpx;
	height: 55rpx;
	line-height: 55rpx;
	text-align: center;
	color: #333333;
	margin-right: 10rpx;
	border-radius: 27.5rpx;
	background-color: #f9e15c;
}
.activity_addr{
	display: flex;
	align-items: center;
}
.activity_addr .picker-box view {
	/* color: #2ea9df; */
	text-align: right;
}
.activity_addr .active-address {
	flex: 1;
	max-width: 520rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #f9e15c;
}
.sponsor_activity .activity_desc{
	padding: 30rpx;
	font-size: 30rpx;
	margin-bottom: 20rpx;
	border-bottom: 1px solid #F6F6F6;
	background-color: #FFFFFF;
}
.sponsor_activity .activity_desc > textarea{
	width: 100%;
	height: 250rpx;
	padding-top: 30rpx;
	font-size: 26rpx;
}
.sponsor_activity .relate_pic{
	padding: 20rpx 30rpx 30rpx;
	font-size: 26rpx;
	margin-bottom: 20rpx;
	background-color: #FFFFFF;
}
.sponsor_activity .relate_pic .up_load{
	width: 120rpx;
	height: 120rpx;
	line-height: 130rpx;
	text-align: center;
	margin-top: 35rpx;
